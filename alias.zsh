# For a full list of active aliases, run `alias`.

# ls
alias ll='ls -l'
alias la='ls -a'
alias lla='ls -la'

# cd
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'

# paths
# I use the prefix "go." for navigation aliases and set these up for
# directories I navigate to often
alias go.desktop="~/Desktop"
alias go.docs="~/Documents"
